{
	"name": "TagCloudChromeEx",
	"version": "0.2.1",
	"manifest_version": 2,

	"description": "Tag Cloud Chrome Extension",
	"icons": {
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": "img/defaultIcon19x19.png",
		"default_popup": "popup.html",
		"default_title": "TagCloudChromeEx"
	},
	"content_scripts": [ {
	"js": [ "js/lib/jquery-1.8.3.min.js", "js/app/content.js" ],
	"matches": [ "*://*/*" ],
	"run_at": "document_start"
	} ],
	"minimum_chrome_version": "18",
	"permissions": [ "http://*/*", "https://*/*", "unlimitedStorage", "contextMenus", "cookies", "tabs", "notifications" ]

}